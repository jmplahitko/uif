{
	"compilerOptions": {
		"target": "es2017",
		"module": "esnext",
		"moduleResolution": "node",
		"lib": [
			"DOM",
			"ESNext"
		],
		"experimentalDecorators": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"useDefineForClassFields": true,
		"declaration": true,
		"declarationMap": true,
	},
	"exclude": [
		"node_modules"
	]
}